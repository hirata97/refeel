{
  "name": "refeel",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite --config config/vite.config.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --config config/vite.config.ts",
    "test:unit": "vitest --config config/vitest.config.ts",
    "test:e2e": "playwright test --config config/playwright.config.ts",
    "build-only": "vite build --config config/vite.config.ts",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint . --fix --config config/eslint.config.js",
    "format": "prettier --write src/",
    "ci:lint": "eslint . --max-warnings=0 --config config/eslint.config.js",
    "ci:test": "vitest run --coverage --reporter=verbose --config config/vitest.config.ts",
    "ci:type-check": "./scripts/ci-type-check.sh",
    "ci:security": "npm audit --audit-level=high",
    "ci:build": "vite build --config config/vite.config.ts",
    "ci:e2e": "playwright test --config config/playwright.config.ts --reporter=html",
    "create-pr": "./scripts/create-pr.sh",
    "fetch-issue": "./scripts/fetch-issue.sh",
    "auto-issue": "./scripts/auto-issue.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs:app": "docker-compose logs -f app",
    "docker:seed": "docker-compose exec supabase-db psql -U postgres -d postgres -f /docker-entrypoint-initdb.d/00000000000000_initial_schema.sql",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "docker:psql": "docker-compose exec supabase-db psql -U postgres -d postgres",
    "generate-types": "node scripts/generate-types.js",
    "generate-types:prod": "node scripts/generate-types.js --production",
    "dev:with-types": "npm run generate-types && npm run dev",
    "ci:all": "npm run generate-types && npm run ci:lint && npm run ci:type-check && npm run ci:test && npm run ci:build && npm run ci:security",
    "ci:quality-gate": "npm run generate-types && npm run ci:lint && npm run ci:type-check && npm run ci:build",
    "ci:minimum-gate": "npm run generate-types && npm run ci:lint && npm run ci:build",
    "ci:ideal-gate": "npm run ci:all"
  },
  "overrides": {
    "glob": "^11.0.4"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.81.1",
    "@vee-validate/i18n": "^4.15.1",
    "@vee-validate/rules": "^4.15.1",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.4.8",
    "dexie": "^4.2.0",
    "dompurify": "^3.2.6",
    "isomorphic-dompurify": "^2.26.0",
    "pinia": "^3.0.4",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.24",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.6.3",
    "vuetify": "^3.7.6"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@playwright/test": "^1.56.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^27.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/eslint-plugin": "^1.4.3",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.14.0",
    "eslint-plugin-playwright": "^2.0.0",
    "eslint-plugin-vue": "^10.5.1",
    "jsdom": "^27.2.0",
    "lodash-es": "^4.17.21",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.3.3",
    "supabase": "^2.58.5",
    "terser": "^5.43.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^4.0.9",
    "vue-tsc": "^3.1.4",
    "workbox-window": "^7.3.0"
  }
}
